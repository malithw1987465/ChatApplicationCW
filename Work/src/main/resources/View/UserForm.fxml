<AnchorPane fx:id="dashboardPane" prefHeight="476.0" prefWidth="686.0" style="-fx-background-color: #95a5a6;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UserFormController">
