            <HBox prefHeight="58.0" prefWidth="665.0">
               <children>
                  <VBox fx:id="vBox" prefHeight="58.0" prefWidth="720.0" />
               </children>
            </HBox>
         </content>
      </ScrollPane>
      <ImageView fx:id="imgimoji" fitHeight="26.0" fitWidth="22.0" layoutX="550.0" layoutY="400.0">
         <image>
            <Image url="@../img/smiling-face.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
