      <ImageView fitHeight="480.0" fitWidth="688.0">
         <image>
            <Image url="@../img/img.png" />
         </image>
      </ImageView>
      <TextField fx:id="txtMessage" layoutX="14.0" layoutY="392.0" prefHeight="38.0" prefWidth="504.0" />
      <Button fx:id="btnSend" layoutX="592.0" layoutY="399.0" mnemonicParsing="false" onAction="#bntSendOnAction" prefHeight="31.0" prefWidth="80.0" style="-fx-background-color: #3498db;" text="Send" />
      <Label fx:id="lblName" layoutX="23.0" layoutY="14.0" prefHeight="56.0" prefWidth="167.0">
         <font>
            <Font size="31.0" />
         </font>
      </Label>
      <Button fx:id="btnCamera" layoutX="454.0" layoutY="393.0" mnemonicParsing="false" onAction="#btnCameraOnAction" prefHeight="36.0" prefWidth="50.0">
